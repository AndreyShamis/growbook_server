{% extends 'base.html.twig' %}

{% block title %}Fertilize index{% endblock %}

{% block body %}
    <h1>Fertilize index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Water</th>
                <th>Ph</th>
                <th>Tds</th>
                <th>Ec</th>
                <th>Temperature</th>
                <th>Amount</th>
                <th>Name</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>Type</th>
                <th>Value</th>
                <th>Note</th>
                <th>Value1</th>
                <th>Value2</th>
                <th>Value3</th>
                <th>Ip</th>

                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for event_fertilize in event_fertilizes %}
            <tr>
                <td>{{ event_fertilize.id }}</td>
                <td>{{ event_fertilize.water }}</td>
                <td>{{ event_fertilize.ph }}</td>
                <td>{{ event_fertilize.tds }}</td>
                <td>{{ event_fertilize.ec }}</td>
                <td>{{ event_fertilize.temperature }}</td>
                <td>{{ event_fertilize.amount }}</td>
                <td>{{ event_fertilize.name }}</td>
                <td>{{ event_fertilize.createdAt ? event_fertilize.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ event_fertilize.updatedAt ? event_fertilize.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ event_fertilize.type }}</td>
                <td>{{ event_fertilize.value }}</td>
                <td>{{ event_fertilize.note }}</td>
                <td>{{ event_fertilize.value1 }}</td>
                <td>{{ event_fertilize.value2 }}</td>
                <td>{{ event_fertilize.value3 }}</td>
                <td>{{ event_fertilize.ip }}</td>
                <td>
                    <a href="{{ path('events_event_fertilize_show', {'id': event_fertilize.id}) }}">show</a>
                    <a href="{{ path('events_event_fertilize_edit', {'id': event_fertilize.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="18">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('events_event_fertilize_new') }}">Create new</a>
{% endblock %}
