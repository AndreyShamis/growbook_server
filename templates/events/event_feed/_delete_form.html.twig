<form method="post" action="{{ path('events_event_feed_delete', {'id': event_feed.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ event_feed.id) }}">
    <button class="btn">Delete</button>
</form>
