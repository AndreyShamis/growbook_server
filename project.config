[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/Andrey Shamis (AndreyShamis)
	read = group user/symfony.growbook_server
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Andrey Shamis (AndreyShamis)
	label-Verified = -1..+1 group user/Andrey Shamis (AndreyShamis)
	label-Verified = -1..+1 group user/symfony.growbook_server
	push = group user/Andrey Shamis (AndreyShamis)
	read = group user/Andrey Shamis (AndreyShamis)
	rebase = group user/Andrey Shamis (AndreyShamis)
	removeReviewer = group user/Andrey Shamis (AndreyShamis)
	submit = group user/Andrey Shamis (AndreyShamis)
[access "refs/heads/*"]
	create = group user/Andrey Shamis (AndreyShamis)
	push = +force group user/Andrey Shamis (AndreyShamis)
	pushMerge = group user/Andrey Shamis (AndreyShamis)
	read = group user/Andrey Shamis (AndreyShamis)
	label-Code-Review = -1..+1 group user/symfony.growbook_server
	label-Verified = -1..+1 group user/symfony.growbook_server
[access "refs/tags/*"]
	push = group user/Andrey Shamis (AndreyShamis)
	push = +force group user/Andrey Shamis (AndreyShamis)
	create = group user/Andrey Shamis (AndreyShamis)
	createSignedTag = group user/Andrey Shamis (AndreyShamis)
	createTag = group user/Andrey Shamis (AndreyShamis)
[receive]
	requireSignedOffBy = true
	requireChangeId = true
